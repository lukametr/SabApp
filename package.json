{
  "name": "sabap",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm run --parallel dev",
    "build": "pnpm run --filter=frontend build && pnpm run --filter=backend build",
    "start": "pnpm run --parallel start",
    "start:prod": "cd apps/backend && pnpm start:prod",
    "lint": "pnpm run --parallel lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "pnpm run --parallel clean",
    "build:prod": "bash scripts/build.sh",
    "start:prod:script": "bash scripts/start.sh"
  },
  "devDependencies": {
    "@types/node": "^20.19.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.11.1",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/serve-static": "4.0.0",
    "@nestjs/swagger": "^7.1.17",
    "@prisma/client": "^6.9.0",
    "@types/archiver": "^6.0.3",
    "@types/bcryptjs": "^3.0.0",
    "@types/compression": "^1.8.1",
    "@types/helmet": "^4.0.0",
    "archiver": "^6.0.1",
    "backend": "^0.0.0",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "cross-env": "^7.0.3",
    "multer": "^1.4.5-lts.1",
    "prisma": "^6.9.0",
    "stripe": "^18.2.1",
    "tsup": "^8.5.0",
    "typeorm": "^0.3.24",
    "uuid": "^9.0.1"
  }
}