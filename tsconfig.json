{
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./apps/frontend/src/*"]
    }
  },
  "exclude": ["node_modules"],
  "include": ["apps/frontend/**/*.ts", "apps/frontend/**/*.tsx"]
} 